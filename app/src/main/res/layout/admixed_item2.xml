<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll"
    android:layout_width="@dimen/carTypeRightWidth"
    android:layout_height="@dimen/ItmeHeightS"
    android:background="@color/common_gray_line"
    android:orientation="vertical"
    >
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/common_gray_line"
        />
    <TextView
        android:id="@+id/tv_item"
        android:layout_width="match_parent"
        android:layout_height="@dimen/ItmeHeight"
        android:textColor="@color/common_theme_blue"
        android:background="@color/white"
        android:gravity="center"
        android:textSize="@dimen/carTypeTextSize"
        />

</LinearLayout>
